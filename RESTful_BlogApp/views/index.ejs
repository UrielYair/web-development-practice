<%- include("partials/header.ejs") %>

<div class="container main">
    <h1>Index Page</h1>

    <% blogs.forEach((blog)=>{ %>
        <div>
            <h2><%= blog.title %></h2>
            <img src="<%= blog.image %>">
            <p><%= blog.created %></p>
            <p><%= blog.body.substring(0, 100) %>...</p>
            <a href="/blogs/<%= blog._id%>"> More info</a>
        </div>
    <% }) %>
</div>

<%- include("partials/footer.ejs") %>
